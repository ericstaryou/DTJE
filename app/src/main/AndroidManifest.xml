    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RestaurantListActivity"
            android:label="@string/title_activity_restaurant_list" >
        </activity>
        <activity
            android:name=".SelectCuisineActivity"
            android:label="@string/title_activity_select_cuisine" >
        </activity>
        <activity
            android:name=".AutoSelectRestaurantActivity"
            android:label="@string/title_activity_auto_select_restaurant" >
        </activity>
        <activity
            android:name=".OrderActivity"
            android:label="@string/title_activity_order">
        </activity>
    </application>

</manifest>
